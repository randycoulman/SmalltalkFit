gory><body package="Fit-Book-DomainCode">cents: anInteger 	^self new cents: anInteger</body><body package="Fit-Book-DomainCode">dollars: aDouble 	^self cents: (aDouble * 100) rounded</body><body package="Fit-Book-DomainCode">fromString: aString 	(aString matchesRegex: '\$[0-9]*(\.[0-9][0-9])?') 		ifFalse: [^self error: 'Invalid money'].	^self dollars: (Number fitParse: (aString copyFrom: 2 to: aString size))</body></methods><methods><class-id>Fitbook.Accounts.Order</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">description	^description</body><body package="Fit-Book-DomainCode">description: anObject	description := anObject</body><body package="Fit-Book-DomainCode">dispatched	^dispatched</body><body package="Fit-Book-DomainCode">dispatched: anObject	dispatched := anObject</body><body package="Fit-Book-DomainCode">partNumber	^partNumber</body><body package="Fit-Book-DomainCode">partNumber: anObject	partNumber := anObject</body><body package="Fit-Book-DomainCode">price	^price</body><body package="Fit-Book-DomainCode">price: anObject	price := anObject</body><body package="Fit-Book-DomainCode">quantity	^quantity</body><body package="Fit-Book-DomainCode">quantity: anObject	quantity := anObject</body><body package="Fit-Book-DomainCode">total	^total</body><body package="Fit-Book-DomainCode">total: anObject	total := anObject</body></methods><methods><class-id>Fitbook.LineItems</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">total	^total</body></methods><methods><class-id>Fitbook.LineItems</class-id> <category>operations</category><body package="Fit-Book-DomainCode">buyFor: aDouble 	total := total + aDouble</body></methods><methods><class-id>Fitbook.LineItems</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">initialize	total := 0.0</body></methods><methods><class-id>Fitbook.LineItems class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">new	^super new initialize</body></methods><methods><class-id>Fitbook.Chat.Room</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">chat	^chat</body><body package="Fit-Book-DomainCode">chat: aChatRoom	chat := aChatRoom</body><body package="Fit-Book-DomainCode">name	^name</body><body package="Fit-Book-DomainCode">name: aString	name := aString</body><body package="Fit-Book-DomainCode">owner: aUser	owner := aUser</body><body package="Fit-Book-DomainCode">users	^users</body></methods><methods><class-id>Fitbook.Chat.Room</class-id> <category>adding</category><body package="Fit-Book-DomainCode">addUser: aUser 	users add: aUser</body><body package="Fit-Book-DomainCode">removeUser: aUser 	users remove: aUser</body></methods><methods><class-id>Fitbook.Chat.Room</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">initialize	users := Set new</body></methods><methods><class-id>Fitbook.Chat.Room class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">named: aString createdBy: aUser for: aChatRoom 	^(self new)		name: aString;		owner: aUser;		chat: aChatRoom;		yourself</body><body package="Fit-Book-DomainCode">new	^super new initialize</body></methods><methods><class-id>Fitbook.Accounts.Invoice</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">accountNumber	^accountNumber</body><body package="Fit-Book-DomainCode">address	^address</body><body package="Fit-Book-DomainCode">customer	^customer</body><body package="Fit-Book-DomainCode">date	^date</body><body package="Fit-Book-DomainCode">delivery	^delivery</body><body package="Fit-Book-DomainCode">orderNumber	^orderNumber</body><body package="Fit-Book-DomainCode">orders	^Array with: ((Order new)				quantity: 1;				partNumber: 'CAT 98142-00-GH';				description: 'L3 Switch 32x1000T';				dispatched: 1;				price: 6804.00;				total: 6804.00;				yourself)		with: ((Order new)				quantity: 2;				partNumber: 'CAT 99000-01-PH';				description: 'Macronetic switch';				dispatched: 2;				price: 2317.00;				total: 2317.00;				yourself)</body><body package="Fit-Book-DomainCode">specialDelivery	^'AS0555P'</body><body package="Fit-Book-DomainCode">total	^self orders inject: 0.0 into: [:sum :each | sum + each total]</body></methods><methods><class-id>Fitbook.Accounts.Invoice</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">initialize	address := #('Uni of Auckland' 'Private Bag 92019' 'Auckland' '').	delivery := #('Attn: Sal Mayha' 'Uni of Auckland' '').	date := '05/01/04'.	accountNumber := '216017-01'.	orderNumber := 'TC000015473-REPL'.	customer := 'UNI34'</body></methods><methods><class-id>Fitbook.Accounts.Invoice class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">new	^super new initialize</body></methods><methods><class-id>Fitbook.Card</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">card: aString	card := aString</body><body package="Fit-Book-DomainCode">suit: aString	suit := aString</body></methods><methods><class-id>Fitbook.Card class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">card: cardString suit: suitString 	^(self new)		card: cardString;		suit: suitString</body></methods><methods><class-id>Fitbook.DiscountForGroup</class-id> <category>operations</category><body package="Fit-Book-DomainCode">discountForFutureValue: aString owing: owingDouble purchase: purchaseDouble 	^(aString = futureValue 		and: [owingDouble &lt;= maxOwing and: [purchaseDouble &gt;= minPurchase]]) 			ifTrue: [purchaseDouble * discountPercent / 100]			ifFalse: [0.0]</body></methods><methods><class-id>Fitbook.DiscountForGroup</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">setFutureValue: aString maxOwing: maxOwingDouble minPurchase: minPurchaseDouble discountPercent: discountPercentDouble	futureValue := aString.	maxOwing := maxOwingDouble.	minPurchase := minPurchaseDouble.	discountPercent := discountPercentDouble</body></methods><methods><class-id>Fitbook.DiscountForGroup class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">futureValue: aString maxOwing: maxOwingDouble minPurchase: minPurchaseDouble discountPercent: discountPercentDouble	^self new		setFutureValue: aString		maxOwing: maxOwingDouble		minPurchase: minPurchaseDouble		discountPercent: discountPercentDouble</body></methods><methods><class-id>Fitbook.DiscountForGroup class</class-id> <category>utility</category><body package="Fit-Book-DomainCode">elements	^#(#('low' 0 0 0) #('low' 0 2000 3) #('medium' 500 600 3) #('medium' 0 500 5) #('high' 2000 2000 10)) 		collect: 			[:each | 			(self new)				futureValue: each first;				maxOwing: (each at: 2);				minPurchase: (each at: 3);				discountPercent: each last;				yourself]</body></methods><methods><class-id>Fitbook.DiscountApplication</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">initialize	groups := OrderedCollection new</body></methods><methods><class-id>Fitbook.DiscountApplication</class-id> <category>operations</category><body package="Fit-Book-DomainCode">discountForFutureValue: aString owing: owingDouble purchase: purchaseDouble 	^(groups collect: 			[:each | 			each 				discountForFutureValue: aString				owing: owingDouble				purchase: purchaseDouble]) 		detect: [:each | each &gt; 0]		ifNone: [0.0]</body></methods><methods><class-id>Fitbook.DiscountApplication</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">groups	^groups</body></methods><methods><class-id>Fitbook.DiscountApplication</class-id> <category>adding</category><body package="Fit-Book-DomainCode">addDiscountGroupFutureValue: aString maxBalance: maxOwingDouble minPurchase: minPurchaseDouble discountPercent: discountPercentDouble	groups add: (DiscountForGroup				futureValue: aString				maxOwing: maxOwingDouble				minPurchase: minPurchaseDouble				discountPercent: discountPercentDouble)</body></methods><methods><class-id>Fitbook.DiscountApplication class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">new	^super new initialize</body></methods><methods><class-id>Fitbook.Credit</class-id> <category>operations</category><body package="Fit-Book-DomainCode">allowsCreditForMonths: anInteger reliable: aBoolean balance: aDouble 	^anInteger &gt; 12 and: [aBoolean and: [aDouble &lt; 6000]]</body><body package="Fit-Book-DomainCode">limitForMonths: anInteger reliable: aBoolean balance: aDouble	^(self allowsCreditForMonths: anInteger reliable: aBoolean balance: aDouble)		ifTrue: [1000.0]		ifFalse: [0.0]</body></methods><methods><class-id>Fitbook.Chat.User</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">setName: aString	name := aString</body></methods><methods><class-id>Fitbook.Chat.User</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">name	^name</body></methods><methods><class-id>Fitbook.Chat.User class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">named: aString	^self new setName: aString</body></methods><methods><class-id>Fitbook.Chat.ChatRoom</class-id> <category>private</category><body package="Fit-Book-DomainCode">createRoom: aString by: aUser 	rooms at: aString		put: (Room 				named: aString				createdBy: aUser				for: self)</body></methods><methods><class-id>Fitbook.Chat.ChatRoom</class-id> <category>initialize-release</category><body package="Fit-Book-DomainCode">initialize	users := Dictionary new.	rooms := Dictionary new</body></methods><methods><class-id>Fitbook.Chat.ChatRoom</class-id> <category>operations</category><body package="Fit-Book-DomainCode">connectUser: aString 	users at: aString put: (User named: aString).	^true</body><body package="Fit-Book-DomainCode">disconnectUser: aString 	| user |	user := self userNamed: aString.	users removeKey: aString.	rooms do: [:each | each removeUser: user].	^true</body><body package="Fit-Book-DomainCode">renameRoom: aRoom to: aString 	rooms		removeKey: aRoom name;		at: aString put: aRoom</body><body package="Fit-Book-DomainCode">user: userNameString createsRoom: roomNameString	| user |	user := self userNamed: userNameString.	user		ifNil: 			[^Error raiseErrorString: ('Unknown user name: &lt;1s&gt;'						expandMacrosWith: userNameString)].	self createRoom: roomNameString by: user.	^true</body><body package="Fit-Book-DomainCode">user: userNameString entersRoom: roomNameString 	| user room |	user := self userNamed: userNameString.	room := self roomNamed: roomNameString.	(user isNil or: [room isNil]) ifTrue: [^false].	room addUser: user.	^true</body></methods><methods><class-id>Fitbook.Chat.ChatRoom</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">roomNamed: aString 	^rooms at: aString ifAbsent: [nil]</body><body package="Fit-Book-DomainCode">rooms	^rooms</body><body package="Fit-Book-DomainCode">userNamed: aString 	^users at: aString ifAbsent: [nil]</body><body package="Fit-Book-DomainCode">usersInRoom: aString 	| room |	room := self roomNamed: aString.	^room users</body></methods><methods><class-id>Fitbook.Chat.ChatRoom class</class-id> <category>instance creation</category><body package="Fit-Book-DomainCode">new	^super new initialize</body></methods><methods><class-id>Fitbook.Sokoban</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">gameBoard	^(OrderedCollection new)		add: 'WWWWWWWWWW';		add: 'WsssessssW';		add: 'WsssbssssW';		add: 'WebssssssW';		add: 'WssssssssW';		add: 'WssssssssW';		add: 'WssssssssW';		add: 'WsswbssseW';		add: 'WsssbssseW';		add: 'WssssssssW';		add: 'WssssssssW';		add: 'WssssssssW';		add: 'WssbsssssW';		add: 'WssesssbeW';		add: 'WssssssssW';		add: 'WssssssssW';		add: 'WWWWWWWWWW';		yourself</body></methods><methods><class-id>Fitbook.DiscountGroup</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">discountPercent: aDouble	discountPercent := aDouble</body><body package="Fit-Book-DomainCode">futureValue: aString	futureValue := aString</body><body package="Fit-Book-DomainCode">maxOwing: aDouble	maxOwing := aDouble</body><body package="Fit-Book-DomainCode">minPurchase: aDouble	minPurchase := aDouble</body></methods><methods><class-id>Fitbook.DiscountGroup class</class-id> <category>utility</category><body package="Fit-Book-DomainCode">elements	^#(#('low' 0 0 0) #('low' 0 2000 3) #('medium' 500 600 3) #('medium' 0 500 5) #('high' 2000 2000 10)) 		collect: 			[:each | 			(self new)				futureValue: each first;				maxOwing: (each at: 2);				minPurchase: (each at: 3);				discountPercent: each last;				yourself]</body></methods><methods><class-id>Fitbook.Discount</class-id> <category>operations</category><body package="Fit-Book-DomainCode">discountFor: aDouble	aDouble &lt; 0 ifTrue: [^self error: 'Can''t be a negative amount'].	^aDouble &lt; 1000 ifTrue: [0.0] ifFalse: [aDouble * 0.05]</body></methods><methods><class-id>Fitbook.Client</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">firstPhone	^phones first</body><body package="Fit-Book-DomainCode">phones: aCollection	phones := aCollection</body></methods><methods><class-id>Fitbook.Chat.Room</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">occupantCount	&lt;fitReturns: #Integer&gt;	^users size</body><body package="Fit-Book-DomainCode">owner	&lt;fitReturns: #String&gt;	^owner name</body></methods><methods><class-id>Fitbook.Chat.Room</class-id> <category>operations</category><body package="Fit-Book-DomainCode">rename: aString	&lt;fitTakes: #String&gt;	chat renameRoom: self to: aString.	name := aString</body></methods><methods><class-id>Fitbook.Card</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">card	&lt;fitReturns: #String&gt;	^card</body><body package="Fit-Book-DomainCode">suit	&lt;fitReturns: #String&gt;	^suit</body></methods><methods><class-id>Fitbook.DiscountForGroup</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">discountPercent	&lt;fitReturns: #Double&gt;	^discountPercent</body><body package="Fit-Book-DomainCode">futureValue	&lt;fitReturns: #String&gt;	^futureValue</body><body package="Fit-Book-DomainCode">maxOwing	&lt;fitReturns: #Double&gt;	^maxOwing</body><body package="Fit-Book-DomainCode">minPurchase	&lt;fitReturns: #Double&gt;	^minPurchase</body></methods><methods><class-id>Fitbook.Chat.ChatRoom</class-id> <category>operations</category><body package="Fit-Book-DomainCode">removeRoom: aString	&lt;fitTakes: #String&gt;	&lt;fitReturns: #Boolean&gt;	| room |	room := self roomNamed: aString.	room occupantCount &gt; 0 ifTrue: [^false].	rooms removeKey: aString.	^true</body><body package="Fit-Book-DomainCode">user: userNameString leavesRoom: roomNameString	&lt;fitTakes: #(#String #String)&gt;	&lt;fitReturns: #Boolean&gt;	| user room |	user := self userNamed: userNameString.	room := self roomNamed: roomNameString.	(user isNil or: [room isNil]) ifTrue: [^false].	room removeUser: user.	^true</body></methods><methods><class-id>Fitbook.Chat.ChatRoom</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">occupants: aString	&lt;fitTakes: #String&gt;	&lt;fitReturns: #Integer&gt;	| room |	room := self roomNamed: aString.	^room occupantCount</body></methods><methods><class-id>Fitbook.DiscountGroup</class-id> <category>accessing</category><body package="Fit-Book-DomainCode">discountPercent	&lt;fitReturns: #Number&gt;	^discountPercent</body><body package="Fit-Book-DomainCode">futureValue	&lt;fitReturns: #String&gt;	^futureValue</body><body package="Fit-Book-DomainCode">maxOwing	&lt;fitReturns: #Number&gt;	^maxOwing</body><body package="Fit-Book-DomainCode">minPurchase	&lt;fitReturns: #Number&gt;	^minPurchase</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><name>Object</name><environment>Core</environment><super></super><private>false</private><indexed-type>none</indexed-type><inst-vars></inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>Kernel-Objects</category><attributes><package>Kernel-Objects</package></attributes></class><class><name>Magnitude</name><environment>Core</environment><super>Core.Object</super><private>false</private><indexed-type>none</indexed-type><inst-vars></inst-vars><class-inst-vars></class-inst-vars><imports></imports><category>Magnitude-General</category><attributes><package>Magnitude-General</package></attributes></class></st-source>